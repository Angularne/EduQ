{
  "compilerOptions": {
    "target": "ES5",
    "module": "system",
    "sourceMap": true,
    "watch": false,
    "outDir": "build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "moduleResolution": "node"
  },
  "compileOnSave": true,
  "filesGlob": [
    "**/*.ts",
    "**/*.tsx",
    "!node_modules/**",
    "!typings/**"
  ],
  "files": [
    "app/app.ts",
    "app/boot.ts",
    "app/common/binding.ts",
    "app/common/headers.ts",
    "app/common/LoggedInOutlet.ts",
    "app/common/range.ts",
    "app/common/reverse.ts",
    "app/components/adminpage/adminpage.ts",
    "app/components/edit.subject/edit.subject.ts",
    "app/components/edit.subject/requirement/requirement.ts",
    "app/components/edit.subject/task/task.ts",
    "app/components/edit.subject/users/users.ts",
    "app/components/edit.user/edit.user.ts",
    "app/components/login/login.ts",
    "app/components/main/main.ts",
    "app/components/me/me.ts",
    "app/components/me/subject/subject.ts",
    "app/components/siteheader/siteheader.ts",
    "app/components/socket/socket.ts",
    "app/components/subject/broadcasts/broadcast/broadcast.ts",
    "app/components/subject/broadcasts/broadcasts.ts",
    "app/components/subject/queue/queue.ts",
    "app/components/subject/subject.ts",
    "app/components/users/userlist/userlist.ts",
    "app/components/users/users.ts",
    "app/interfaces/location.ts",
    "app/interfaces/subject.ts",
    "app/interfaces/user.ts",
    "app/services/auth.service.ts",
    "app/services/broadcast.service.ts",
    "app/services/location.service.ts",
    "app/services/queue.service.ts",
    "app/services/socket.service.ts",
    "app/services/subject.service.ts",
    "app/services/user.service.ts"
  ],
  "atom": {
    "rewriteTsconfig": true
  }
}
